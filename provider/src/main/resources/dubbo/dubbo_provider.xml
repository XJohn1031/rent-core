<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans-4.3.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="rent-core"/>

	<dubbo:registry
			client="curator"
			protocol="zookeeper"
			id="rent-core-registry"
			check="false"
			register="true"
			address="127.0.0.1:2181"/>

	<dubbo:protocol name="dubbo" port="20880"/>

	<dubbo:service interface="cn.bupt.john.remote.TestRemote" ref="testRemoteImpl" register="true"/>


</beans>